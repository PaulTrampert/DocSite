{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "embed": "Templates/**/*",
    "compile": "**/*.cs"
  },
  "packOptions": {
    "projectUrl": "https://github.com/PaulTrampert/DocSite",
    "licenseUrl": "https://opensource.org/licenses/MIT",
    "owners": [
      "Paul Trampert"
    ],
    "tags": [
      "documentation",
      "generator"
    ],
    "summary": "Builds documentation pages out of C# documentation comments."
  },
  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0"
    },
    "System.Xml.XmlDocument": "4.0.1",
    "System.Xml.XmlSerializer": "4.0.11"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50"
    }
  }
}
